<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/paitent_nav_graph"
    app:startDestination="@id/symptoms">

    <fragment
        android:id="@+id/symptoms"
        android:name="com.example.khushi_baby.Fragments.symptoms"
        android:label="fragment_symptoms"
        tools:layout="@layout/fragment_symptoms" >
        <action
            android:id="@+id/action_symptoms_to_diagnosis"
            app:destination="@id/diagnosis" />
    </fragment>
    <fragment
        android:id="@+id/diagnosis"
        android:name="com.example.khushi_baby.Fragments.Diagnosis"
        android:label="fragment_diagnosis"
        tools:layout="@layout/fragment_diagnosis" >
        <action
            android:id="@+id/action_diagnosis_to_prescription"
            app:destination="@id/prescription" />
    </fragment>
    <fragment
        android:id="@+id/prescription"
        android:name="com.example.khushi_baby.Fragments.Prescription"
        android:label="fragment_prescription"
        tools:layout="@layout/fragment_prescription" />
</navigation>